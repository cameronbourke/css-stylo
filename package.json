{
  "name": "stylo",
  "version": "0.0.1",
  "description": "'stylo takes any css string and converts it to a style object'",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --hot --content-base example/ --config webpack.config.js",
    "build": "webpack",
    "compile": "babel src --out-dir dist",
    "prepublish": "npm run compile",
    "test": "tape -r babel-register ./src/**/*.spec.js | tap-spec"
  },
  "keywords": [
    "react",
    "css",
    "style"
  ],
  "devDependencies": {
    "babel-core": "~6.3.26",
    "babel-loader": "~6.2.0",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-preset-stage-0": "~6.3.13",
    "expect": "~1.13.4",
    "react": "0.14.7",
    "react-ace": "3.1.0",
    "react-addons-test-utils": "~0.14.3",
    "react-dom": "~0.14.5",
    "tap-spec": "~4.1.1",
    "tape": "~4.4.0",
    "webpack": "~1.12.9",
    "webpack-dev-server": "~1.14.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cameronbourke/stylo.git"
  },
  "author": "Cameron Bourke <cameron2295@gmail.com> (http://cameronbourke.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cameronbourke/stylo/issues"
  }
}
